{
	"name": "@buildos/web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:fast": "vite dev --mode development",
		"dev:host": "vite dev --host",
		"dev:https": "vite dev --https",
		"dev:debug": "DEBUG=vite:* vite dev",
		"dev:clean": "if exist .svelte-kit rmdir /s /q .svelte-kit && if exist node_modules\\.vite rmdir /s /q node_modules\\.vite && pnpm run dev",
		"dev:split": "concurrently \"pnpm run dev:fast\" \"pnpm run check:watch\"",
		"dev:profile": "NODE_OPTIONS='--inspect' pnpm run dev",
		"build": "vite build",
		"build:prod": "NODE_ENV=production vite build",
		"build:analyze": "ANALYZE=true vite build",
		"build:log": "node scripts/build-logger.js",
		"build:verbose": "vite build --logLevel info",
		"build:metrics": "node scripts/build-metrics.js",
		"preview": "vite preview",
		"preview:host": "vite preview --host",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --watch",
		"format": "prettier --write .",
		"lint": "eslint . --ext .ts,.js,.svelte",
		"lint:fix": "eslint . --ext .ts,.js,.svelte --fix && pnpm run format",
		"test": "vitest run",
		"test:quiet": "vitest run --reporter=dot",
		"test:silent": "VITEST_SILENT=true vitest run --reporter=dot 2>/dev/null || true",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:llm": "vitest run --config vitest.config.llm.ts",
		"test:llm:watch": "vitest --config vitest.config.llm.ts",
		"test:llm:ui": "vitest --ui --config vitest.config.llm.ts",
		"test:llm:verbose": "vitest run --config vitest.config.llm.ts --reporter=verbose",
		"test:llm:fast": "vitest run --config vitest.config.llm.ts --reporter=dot",
		"gen:embeddings": "tsx scripts/generate-embeddings.ts",
		"gen:ctx": "tsx generateProjectContext.ts . -o ./project-context.json",
		"gen:sctx": "tsx scripts/generateStreamlinedProjectContext.ts . -o ./docs/development/sproject-context.json",
		"gen:types": "tsx scripts/generate-types.ts",
		"gen:blog-context": "tsx scripts/generate-blog-context.ts",
		"gen:sitemap": "tsx scripts/generate-sitemap.ts",
		"gen:schema": "tsx scripts/extract-database-schema.ts",
		"gen:all": "pnpm run gen:types && pnpm run gen:blog-context && pnpm run gen:sitemap && pnpm run gen:sctx && pnpm run format  && pnpm run gen:schema ",
		"gen:api-docs": "tsx scripts/generate-api-docs.ts",
		"merge:comments": "tsx scripts/merge-comments.ts",
		"gen:route-docs": "tsx scripts/generate-route-docs.ts",
		"docs:api": "pnpm run gen:api-docs && pnpm run gen:route-docs",
		"docs:serve": "vite preview --open /docs/api",
		"generate-configs": "tsx scripts/generate-field-configs.ts",
		"deps:check": "pnpm outdated",
		"deps:update": "pnpm update --interactive",
		"deps:audit": "pnpm audit",
		"deps:fix": "pnpm audit --fix",
		"clean": "if exist .svelte-kit rmdir /s /q .svelte-kit && if exist build rmdir /s /q build && if exist node_modules\\.vite rmdir /s /q node_modules\\.vite && if exist .tsbuildinfo del .tsbuildinfo",
		"clean:all": "pnpm run clean && if exist node_modules rmdir /s /q node_modules",
		"pre-push": "concurrently \"pnpm run check\" \"pnpm run test\" \"pnpm run lint\" && pnpm run build:prod",
		"prepare": "husky",
		"migrate:context": "tsx scripts/migrate-project-context.ts",
		"migrate:usercontext": "tsx scripts/migrate-parsed-to-input.js",
		"test:security": "node scripts/test-security.js"
	},
	"devDependencies": {
		"@babel/parser": "^7.28.0",
		"@babel/traverse": "^7.28.0",
		"@babel/types": "^7.28.0",
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.32.0",
		"@obele-michael/swagger-ui-svelte": "^0.0.5",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-vercel": "^5.8.0",
		"@sveltejs/kit": "^2.31.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"@types/sanitize-html": "^2.16.0",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^8.38.0",
		"@typescript-eslint/parser": "^8.38.0",
		"@vitest/ui": "^3.2.4",
		"@xenova/transformers": "^2.17.2",
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"autoprefixer": "^10.4.21",
		"concurrently": "^9.2.0",
		"dotenv": "^17.2.1",
		"dotenv-cli": "^10.0.0",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.0.8",
		"eslint-plugin-svelte": "^3.11.0",
		"fast-glob": "^3.3.3",
		"globals": "^16.3.0",
		"happy-dom": "^18.0.1",
		"husky": "^9.0.0",
		"jsdom": "^26.1.0",
		"lint-staged": "^16.1.2",
		"mdsvex": "^0.12.3",
		"node-fetch": "^3.3.2",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"remark-gfm": "^4.0.0",
		"remark-toc": "^9.0.0",
		"rollup-plugin-visualizer": "^6.0.3",
		"svelte": "^5.37.2",
		"svelte-check": "^4.3.0",
		"tailwindcss": "^3.4.0",
		"tsx": "^4.20.3",
		"typescript": "^5.9.2 ",
		"vite": "^7.0.6",
		"vite-plugin-compression": "^0.5.1",
		"vitest": "^3.2.4",
		"yaml": "^2.3.4",
		"zod": "^3.23.0"
	},
	"engines": {
		"node": ">=20.19.0",
		"pnpm": ">=8.0.0"
	},
	"dependencies": {
		"@buildos/shared-types": "workspace:*",
		"@buildos/supabase-client": "workspace:*",
		"@buildos/twilio-service": "workspace:*",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.53.0",
		"@tiptap/core": "^3.0.9",
		"@tiptap/extension-color": "^3.0.9",
		"@tiptap/extension-image": "^3.0.9",
		"@tiptap/extension-link": "^3.0.9",
		"@tiptap/extension-text-align": "^3.0.9",
		"@tiptap/extension-text-style": "^3.0.9",
		"@tiptap/starter-kit": "^3.0.9",
		"@types/canvas-confetti": "^1.9.0",
		"@types/node": "^24.1.0",
		"@types/nodemailer": "^6.4.17",
		"@vercel/analytics": "^1.5.0",
		"@vercel/speed-insights": "^1.2.0",
		"canvas-confetti": "^1.9.3",
		"date-fns": "^4.1.0",
		"date-fns-tz": "^3.2.0",
		"google-auth-library": "^10.2.0",
		"googleapis": "^154.1.0",
		"lucide-svelte": "^0.536.0",
		"marked": "^16.1.1",
		"mode-watcher": "^1.1.0",
		"nodemailer": "^7.0.5",
		"openai": "^5.11.0",
		"sanitize-html": "^2.17.0",
		"sharp": "^0.34.3",
		"stripe": "^18.4.0",
		"sveltekit-api": "^0.6.0",
		"swagger-ui-dist": "^5.29.0",
		"tailwind-merge": "^3.3.1",
		"ts-morph": "^27.0.0",
		"twilio": "^4.23.0",
		"uuid": "^11.1.0"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,css}": [
			"prettier --write"
		]
	}
}
